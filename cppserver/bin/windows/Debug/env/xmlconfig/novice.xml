    <novice id="648.000000" content="101250.000000" content_remarks="传闻深渊之蛇每次出动时都会怀揣着大量的宝藏，只要我们击败它获得奖励，重建小镇恢复昔日的辉煌就指日可待啦！" pic="" dialog="1.000000" type="0.000000" click_control="" description_y="25.000000" keywords="0.000000" keywords_content="" is_important_step="" genie_id="16105.000000" genie_name="九尾狐" genie_res="ui/png/genie_novice/403_ninetailsfox.png" special_event="0.000000" />
    <novice id="660.000000" content="101251.000000" content_remarks="勇士，你解锁了试炼之境，快去看看吧！" pic="" dialog="1.000000" type="0.000000" click_control="" description_y="25.000000" keywords="0.000000" keywords_content="" is_important_step="" genie_id="16105.000000" genie_name="九尾狐" genie_res="ui/png/genie_novice/403_ninetailsfox.png" special_event="0.000000" />
